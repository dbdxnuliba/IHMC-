<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="102.0" prefWidth="167.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.humanoidBehaviors.ui.behaviors.FancyPosesBehaviorUIController">
    <children>
        <GridPane hgap="10.0" layoutX="15.0" layoutY="17.0" vgap="5.0">
            <children>
            <HBox>
               <children>
                  <CheckBox fx:id="enableCheckBox" mnemonicParsing="false" onAction="#enable" text="Enable" />
               </children>
            </HBox>
                <Button fx:id="singleSupportButton" onAction="#requestSingleSupport" text="SingleSupport" GridPane.rowIndex="1" />
            <Button fx:id="runningManButton" onAction="#requestRunningMan" text="RunningMan" GridPane.rowIndex="2" />
                <Button fx:id="karateKid1Button" onAction="#requestKarateKid1" text="KarateKid1" GridPane.rowIndex="3" />
            <Button fx:id="karateKid2Button" onAction="#requestKarateKid2" text="KarateKid2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="karateKid3Button" onAction="#requestKarateKid3" text="KarateKid3" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="presentButton" onAction="#requestPresent" text="Present" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="shutdownPoseButton" onAction="#requestShutdownPose" text="ShutdownPose" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            </children>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
         </rowConstraints>
        </GridPane>
    </children>
</AnchorPane>
